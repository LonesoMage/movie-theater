{
  "name": "movie-theater",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "cypress:open": "cypress open --config-file cypress.config.mts",
    "cypress:run": "cypress run --config-file cypress.config.mts",
    "cypress:run:headless": "cypress run --headless --config-file cypress.config.mts",
    "cypress:run:chrome": "cypress run --browser chrome --config-file cypress.config.mts",
    "cypress:run:firefox": "cypress run --browser firefox --config-file cypress.config.mts",
    "test:e2e": "start-server-and-test dev http://localhost:5173 cypress:run",
    "test:e2e:open": "start-server-and-test dev http://localhost:5173 cypress:open",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173 cypress:run:headless",
    "e2e": "npm run test:e2e",
    "e2e:open": "npm run test:e2e:open"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.2",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^6.0.3",
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/ui": "^3.2.3",
    "cypress": "^14.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "start-server-and-test": "^2.0.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.3",
    "wait-on": "^8.0.3"
  }
}